<% if @errors %>
  <%= render partial: '/shared/errors' %>
<% end %>

<%= form_for User.new do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit('register') %>
<% end %>
